@model RSSFeeds.Models.RSSFeed

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>RSSFeed</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.RSSFeedTitle)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.RSSFeedTitle)
            @Html.ValidationMessageFor(model => model.RSSFeedTitle)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.RSSFeedUrl)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.RSSFeedUrl)
            @Html.ValidationMessageFor(model => model.RSSFeedUrl)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.UserId)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.UserId)
            @Html.ValidationMessageFor(model => model.UserId)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    <b>
        @Html.ActionLink("Back to Feed List", "List")
    </b>
</div>
