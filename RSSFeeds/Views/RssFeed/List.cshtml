@model IEnumerable<RSSFeeds.Models.RSSFeed>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RSSFeedTitle)
        </th>
        <th></th>
    </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.ActionLink(item.RSSFeedTitle, "List", "RssFeedItems", new { rssFeedUrl = item.RSSFeedUrl }, null)
            </td>
            <td>
                @Html.ActionLink("Edit", "EditFeed", new { rssFeedUrl = item.RSSFeedUrl }) |
                @Html.ActionLink("Delete", "DeleteFeed", new { rssFeedUrl = item.RSSFeedUrl })
            </td>
        </tr>
    }
</table>


<p>
    <b>
        @Html.ActionLink("Add a feed to the list", "AddNewRssFeed")
    </b>
</p>